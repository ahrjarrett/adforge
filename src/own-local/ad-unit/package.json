{
  "name": "@own-local/ad-unit",
  "version": "1.0.2",
  "publishConfig": {
    "access": "restricted"
  },
  "description": "The ares ad unit. Just the ad unit.",
  "main": "lib/AdUnit.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:ts && npm run build:js",
    "build:ts": "tsc --emitDeclarationOnly",
    "build:ts:watch": "npm run build:ts --watch",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "build:js:watch": "npm run build:js --watch",
    "coverage:ci": "JEST_JUNIT_OUTPUT=reports/test-coverage.xml npm run test -- --coverage --no-cache --maxWorkers=6 --ci",
    "coverage": "npm run test -- --coverage --no-cache --maxWorkers=6",
    "fresh": "rm -rf node_modules reports lib",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "deploy:branch": "echo \"AD UNIT --> No deploys for the ad unit\"",
    "deploy:staging": "echo \"AD UNIT --> No deploys for the ad unit\"",
    "deploy:prod": "echo \"AD UNIT --> No deploys for the ad unit\"",
    "test:ci": "echo \"AD UNIT --> No tests for the ad unit\"",
    "test:watch": "echo \"AD UNIT --> No tests for the ad unit\"",
    "test": "echo \"AD UNIT --> No tests for the ad unit\""
  },
  "author": "Alex Maingot (maingot@ownlocal.com)",
  "dependencies": {
    "@own-local/ad-utils": "^1.0.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "extract-css-chunks-webpack-plugin": "^3.1.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "sinon": "^6.3.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "testRegex": "src/.*.test.(ts|tsx|js)$",
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/setupTests.ts",
    "testURL": "http://localhost/",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  },
  "gitHead": "41d2dd6bcb92462d455c26afa004df24e96a8ead"
}
